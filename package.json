{
  "name": "maverick-multi-agent-system",
  "version": "1.0.0",
  "description": "A multi-agent AI system for building complete software projects from natural language descriptions",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon backend/server.js",
    "dev:frontend": "npx live-server client/public",
    "build": "npm run build:css && npm run copy-assets",
    "build:css": "npx tailwindcss -i ./client/src/css/tailwind.css -o ./client/public/styles.css --minify",
    "build:watch": "npx tailwindcss -i ./client/src/css/tailwind.css -o ./client/public/styles.css --watch",
    "copy-assets": "cp -r client/src/assets client/public/ && cp client/src/js/* client/public/ && cp client/src/*.html client/public/",
    "test": "jest",
    "test:integration": "node scripts/test-integration.js",
    "test:multi-agent": "node scripts/test-multi-agent.js",
    "db:migrate": "knex --knexfile config/knexfile.js migrate:latest",
    "db:seed": "knex --knexfile config/knexfile.js seed:run",
    "db:reset": "knex --knexfile config/knexfile.js migrate:rollback --all && npm run db:migrate && npm run db:seed",
    "db:status": "knex --knexfile config/knexfile.js migrate:status",
    "lint": "eslint backend/ client/ --ext .js",
    "lint:fix": "eslint backend/ client/ --ext .js --fix",
    "clean": "rm -rf node_modules && npm install",
    "postinstall": "npm run build:css"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "child_process": "^1.0.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs-extra": "^11.3.0",
    "knex": "^3.1.0",
    "mongoose": "^7.5.0",
    "socket.io": "^4.7.2",
    "sqlite3": "^5.1.6",
    "tailwindcss": "^3.3.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.15",
    "concurrently": "^8.2.1",
    "eslint": "^8.45.0",
    "jest": "^29.6.4",
    "live-server": "^1.2.2",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.29"
  },
  "keywords": [
    "ai",
    "multi-agent",
    "goose",
    "cli",
    "automation",
    "code-generation",
    "maverick"
  ],
  "author": "Maverick Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/maverick"
  }
}
